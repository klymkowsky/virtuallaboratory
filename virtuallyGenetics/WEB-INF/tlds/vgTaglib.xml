<?xml version="1.0" encoding="ISO-8859-1" ?><!DOCTYPE taglib	PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"	"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd"><taglib>	<tlibversion>1.0</tlibversion>	<jspversion>1.1</jspversion>	<shortname>vg</shortname>	<info>virtuallyGenetics tablib</info>	<tag>		<name>mendelLesson</name>		<tagclass>arahn.vg.content.mendel.MendelLesson</tagclass >		<info>Generates a genetics flash applet lesson</info>		<attribute><name>mask</name></attribute>		<attribute><name>name</name></attribute>		<attribute><name>maxGenerations</name></attribute>	</tag>	<tag>		<name>parental</name>		<tagclass >arahn.vg.content.ParentalTag</tagclass >		<info>A parental strain</info>		<attribute><name>name</name></attribute>		<attribute><name>genotype</name></attribute>	</tag>	<tag>		<name>morganLesson</name>		<tagclass>arahn.vg.content.morgan.MorganLesson</tagclass >		<info>Generates a genetics flash applet lesson</info>		<attribute><name>mask</name></attribute>		<attribute><name>name</name></attribute>		<attribute><name>maxGenerations</name></attribute>	</tag>	<tag>		<name>tutorial</name>		<tagclass>arahn.vg.content.TutorialTag</tagclass>		<info>Specifies tutorial for lesson</info>	</tag></taglib>